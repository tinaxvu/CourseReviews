<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.virginia.sde.reviews.MyReviewsController">
    <Label layoutX="517.0" layoutY="-14.0" prefHeight="100.0" prefWidth="245.0" text="My Reviews" textFill="#252525">
        <font>
            <Font name="SansSerif Bold" size="32.0" />
        </font>
    </Label>

    <TableView fx:id="reviewTable" layoutX="89.0" layoutY="100.0" prefHeight="540.0" prefWidth="1100.0">
        <columns>
            <TableColumn fx:id="courseMnemonicColumn" prefWidth="366.6666666666667" text="Course Mnemonic" />
            <TableColumn fx:id="courseNumberColumn" prefWidth="366.6666666666667" text="Course Number" />
            <TableColumn fx:id="courseRatingColumn" prefWidth="366.6666666666667" text="Rating" />
        </columns>
    </TableView>

    <Button fx:id="backButton" layoutX="32.0" layoutY="20.0" mnemonicParsing="false" onAction="#handleBackButton" text="Back" />

    <Label fx:id="successLabel" layoutX="500.0" layoutY="660.0" text="Success" textFill="#06f417" visible="false" />
    <Label fx:id="errorLabel" layoutX="500.0" layoutY="660.0" text="Error" textFill="#f60200" visible="false" />
</AnchorPane>
